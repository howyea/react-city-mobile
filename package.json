{
  "name": "react-city-mobile",
  "version": "0.2.2",
  "description": "This is a component that selects cities about react.",
  "main": "build/index.js",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    }
  },
  "scripts": {
    "build": "NODE_ENV=development babel lib -d build",
    "lint": "eslint lib/**; exit 0",
    "lint:watch": "esw -w lib/**",
    "prepublish": "npm run build-dev",
    "build-dev": "./node_modules/.bin/babel lib --copy-files --source-maps --extensions .es6,.es,.jsx --out-dir build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huiyuye/react-city-mobile.git"
  },
  "author": "ajiu <yhynew@126.com> (https://github.com/huiyuye)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huiyuye/react-city-mobile/issues"
  },
  "homepage": "https://github.com/huiyuye/react-city-mobile#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.2.3"
  },
  "dependencies": {
    "rc-queue-anim": "^1.5.0",
    "rc-scroll-anim": "^2.4.1"
  }
}
